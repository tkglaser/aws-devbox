    "@types/superagent": "~4.1.17",
    "aws-cdk": "^2",
    "jest": "~29.5.0",
    "ts-jest": "~29.0.5",
    "ts-node": "~10.9.1",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "~3.641.0",
    "@aws-sdk/credential-provider-node": "~3.637.0",
    "aws-cdk-lib": "^2",
    "constructs": "^10.0.0",
    "cross-spawn": "~7.0.3",
    "dotenv": "~16.0.3",
    "lodash": "~4.17.21",
    "source-map-support": "~0.5.21",
