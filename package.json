{
  "name": "devbox",
  "version": "0.1.0",
  "scripts": {
    "start": "npm run pre-deployment && npm run bootstrap && npm run deploy && npm run post-deployment && npm run sethosts",
    "stop": "ts-node -r dotenv/config scripts/stop.ts",
    "bootstrap": "ts-node ./scripts/bootstrap.ts",
    "destroy": "ts-node scripts/destroy.ts",
    "sethosts": "sudo npx ts-node -r dotenv/config scripts/set-hosts.ts",
    "pre-deployment": "ts-node ./scripts/pre-deployment.ts",
    "deploy": "ts-node scripts/deploy.ts",
    "post-deployment": "ts-node ./scripts/post-deployment.ts",
    "test": "jest",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "18.14.6",
    "@types/superagent": "^4.1.17",
    "aws-cdk": "2.78.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.327.0",
    "@aws-sdk/credential-provider-node": "^3.327.0",
    "aws-cdk-lib": "2.78.0",
    "constructs": "^10.0.0",
    "cross-spawn": "^7.0.3",
    "dotenv": "^16.0.3",
    "lodash": "^4.17.21",
    "source-map-support": "^0.5.21",
    "superagent": "^8.0.9"
  }
}
